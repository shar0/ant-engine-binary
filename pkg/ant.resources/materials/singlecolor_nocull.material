fx:
  shader_type: PBR
  vs_code: "#include \"default/utils.sh\"\nvec4 CUSTOM_VS_POSITION(VSInput i, inout Varyings v, out mat4 m){return custom_vs_position(i, v, m);}"
  fs_code: "void CUSTOM_FS(Varyings v, out FSOutput o){o.color = u_color;}"
  setting:
    lighting: off
    position_only: true
  varyings:
    a_position: vec3
    v_posWS: "vec3 TEXCOORD0"
properties:
  u_color: {1, 1, 1, 1}
state:
  ALPHA_REF: 0
  CULL: NONE
  DEPTH_TEST: ALWAYS
  MSAA: true
  BLEND: ALPHA
  WRITE_MASK: RGBA