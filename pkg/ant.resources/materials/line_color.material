fx:
  shader_type: PBR
  fs_code: "void CUSTOM_FS(Varyings varyings, out FSOutput fsoutput){ fsoutput.color = u_color; }"
  setting: /pkg/ant.resources/settings/nolighting.ant
  varyings:
    a_position: vec3
properties:
  u_color: {0, 0, 0, 1}
state:
  ALPHA_REF: 1
  CULL: CCW
  DEPTH_TEST: ALWAYS
  LINEAA: true
  MSAA: true
  PT: LINES
  WRITE_MASK: RGBA